{
  "name": "wave",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "tsc && eslint . --ext .js,.jsx,.ts,.tsx --cache",
    "android:local": "npx react-native run-android --variant=localdebug",
    "android:dev": "npx react-native run-android --variant=developmentdebug",
    "android:prod": "npx react-native run-android --variant=productiondebug",
    "android:staging": "npx react-native run-android --variant=stagingdebug",
    "ios:local": "ENVFILE=.env.local && npx react-native run-ios --scheme 'Local'",
    "ios:dev": "ENVFILE=.env.development && npx react-native run-ios --scheme 'Development'",
    "ios:staging": "ENVFILE=.env.development && npx react-native run-ios --scheme 'Staging'",
    "ios:prod": "ENVFILE=.env.production && npx react-native run-ios --scheme 'Production'",
    "build:apk:dev": "cd ./android && ./gradlew clean app:assembleDevelopmentRelease && cd ..",
    "build:apk:staging": "cd ./android && ./gradlew clean app:assembleStagingRelease && cd ..",
    "build:apk:prod": "cd ./android && ./gradlew clean app:assembleProductionRelease && cd ..",
    "build:aab:dev": "cd ./android && ./gradlew clean app:bundleProductionDebug && cd ..",
    "build:aab:prod": "cd ./android && ./gradlew clean app:bundleProductionRelease && cd ..",
    "clean": "cd android && ./gradlew clean && cd ..",
    "logcat": "adb -s emulator-5554 logcat | grep \"NODEJS-MOBILE\\|NODEJS-RN\\|AndroidRuntime\\|ReactNative\"",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.5",
    "@react-native-community/netinfo": "8.2.0",
    "@react-native-masked-view/masked-view": "^0.2.6",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/core": "^6.2.1",
    "@react-navigation/material-top-tabs": "^6.2.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/stack": "^6.2.1",
    "@realm/react": "^0.3.2",
    "axios": "^0.27.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "expo": "^45.0.0",
    "i18next": "^21.8.5",
    "moment": "^2.29.3",
    "react": "17.0.2",
    "react-hook-form": "^7.39.5",
    "react-i18next": "^11.16.9",
    "react-native": "0.68.2",
    "react-native-config": "^1.4.5",
    "react-native-curved-bottom-bar": "^2.1.4",
    "react-native-device-info": "^10.3.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.4.2",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-modal": "^13.0.1",
    "react-native-reanimated": "^2.8.0",
    "react-native-safe-area-context": "4.2.5",
    "react-native-screens": "3.13.1",
    "react-native-skeleton-placeholder": "^5.0.0",
    "react-native-svg": "^13.6.0",
    "react-redux": "^8.0.1",
    "realm": "11.0.0-rc.0",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1",
    "redux-tools": "^3.2.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.67.3",
    "@types/react-redux": "^7.1.24",
    "@types/react-test-renderer": "^17.0.1",
    "@types/redux-logger": "^3.0.9",
    "@types/yup": "^0.29.14",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "eslint-config-react-native-wcandillon": "^3.8.0",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.67.0",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
